{
  "vendor": "Exherbo",
  "product": "Varlink Test System",
  "version": "1",
  "url": "https://github.com/bl33pbl0p/exherbo-varlink",
  "services": [
    {
      "address": "unix:/run/io.systemd.devices;mode=0666",
      "interfaces": [
        "io.systemd.devices"
      ],
      "executable": {
        "path": "/usr/bin/io.systemd.devices"
      }
    },
    {
      "address": "unix:/run/io.systemd.journal;mode=0666",
      "interfaces": [
        "io.systemd.journal"
      ],
      "executable": {
        "path": "/usr/bin/io.systemd.journal"
      }
    },
    {
      "address": "unix:/run/io.systemd.network;mode=0666",
      "interfaces": [
        "io.systemd.network"
      ],
      "executable": {
        "path": "/usr/bin/io.systemd.network"
      }
    },
    {
      "address": "unix:/run/io.systemd.sysinfo;mode=0666",
      "interfaces": [
        "io.systemd.sysinfo"
      ],
      "executable": {
        "path": "/usr/bin/io.systemd.sysinfo"
      }
    },
    {
      "address": "unix:/run/org.kernel.kmod;mode=0666",
      "interfaces": [
        "org.kernel.kmod"
      ],
      "executable": {
        "path": "/usr/bin/org.kernel.kmod"
      }
    },
    {
      "address": "unix:/run/org.varlink.resolver;mode=0666",
      "interfaces": [
        "org.varlink.resolver",
        "org.varlink.activator"
      ]
    },
    {
      "address": "unix:/run/com.redhat.system.accounts;mode=0666",
      "interfaces": [
        "com.redhat.system.accounts"
      ],
      "executable": {
        "path": "/usr/lib/com.redhat.system/accounts.py"
      }
    }
  ]
}
